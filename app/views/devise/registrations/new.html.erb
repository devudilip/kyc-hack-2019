<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

					<div class="wrap-input100 validate-input m-b-16" data-validate="Please enter email">
						<%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "input100", :placeholder=>"Email" %>
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Please enter password">
						<!--<input class="input100" type="password" name="pass" placeholder="Password">-->
              <%= f.password_field :password, autocomplete: "new-password", :class=>"input100",:placeholder=>"Password" %>
						<span class="focus-input100"></span>
					</div><br/>

					<div class="wrap-input100 validate-input m-b-16" data-validate="Please enter password">
						<!--<input class="input100" type="text" name="username" placeholder="Username">-->
						<%= f.password_field :password_confirmation, autofocus: true, autocomplete: "password", :class=>"input100", :placeholder=>"Password" %>
						<span class="focus-input100"></span>
					</div>

					<div class="container-login100-form-btn">
							<%= f.submit "Sign up", :class=>"login100-form-btn" %>
					</div>


<% end %>

<%= render "devise/shared/links" %>
