<table class="data_table sortable">
  <tr>
    <th>Budget</th>
    <th>Login</th>
    <th>Created_at</th>
  </tr>

<% for budget in @budgets %>
 <% if (budget.user_id == current_user.id) %>
 <tr>
     <td><%= budget.budget_price%></td>
     <td><%= current_user.login%></td>
     <td><%= budget.created_at%></td>
 </tr>
 <%end%>
<% end %>

</table>